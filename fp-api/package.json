{
	"name": "fp-api",
	"version": "0.0.0",
	"private": true,
	"main": "dist/server.js",
	"scripts": {
		"start": "node dist/server.js",
		"dev": "concurrently -k -p \"[{name}]\" -n \"Build,App\" -c \"yellow.bold,cyan.bold\" \"npm run watch\" \"npm run server\"",
		"server": "nodemon dist/server.js",
		"build": "tsc",
		"watch": "tsc --watch",
		"db:start": "docker-compose up -d",
		"db:shutdown": "docker-compose stop && docker-compose rm -f",
		"seed": "node src/database/seed/user.js",
		"test": "jest",
		"test:dev": "jest --silent=false --verbose=true --watch",
		"prettier": "npx prettier --write \"src/**/*\" \"test/**/*\""
	},
	"dependencies": {
		"bcrypt": "^3.0.6",
		"cookie-parser": "~1.4.4",
		"cors": "^2.8.5",
		"debug": "~2.6.9",
		"express": "~4.16.1",
		"http-errors": "~1.6.3",
		"jade": "~1.11.0",
		"jsonwebtoken": "^8.5.1",
		"mongoose": "^5.7.7",
		"morgan": "~1.9.1"
	},
	"devDependencies": {
		"@shelf/jest-mongodb": "^1.1.3",
		"@types/dotenv": "^8.2.0",
		"@types/express": "^4.17.3",
		"@types/mongoose": "^5.7.3",
		"@types/node": "^13.9.0",
		"concurrently": "^5.1.0",
		"dotenv": "^8.2.0",
		"eslint": "^6.6.0",
		"eslint-plugin-jest": "^23.1.1",
		"eslint-plugin-prettier": "^3.1.1",
		"faker": "^4.1.0",
		"husky": "^4.2.3",
		"jest": "^24.9.0",
		"lint-staged": "^10.0.8",
		"nodemon": "^1.19.4",
		"prettier": "^1.18.2",
		"supertest": "^4.0.2",
		"typescript": "^3.8.3"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.js": "eslint --cache --fix"
	}
}
