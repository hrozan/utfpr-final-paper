{
  "name": "fp-server",
  "version": "0.0.0",
  "private": true,
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon -r dotenv/config src/index.ts",
    "server": "node -r dotenv/config dist/server.js",
    "server:watch": "nodemon -r dotenv/config dist/server.js",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "seed": "ts-node -r dotenv/config scripts/seed.ts",
    "test": "jest",
    "test:integration": "jest -c jest.config.integration.js",
    "test:integration:watch": "jest -c jest.config.integration.js --watch --silent=false --verbose=true",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch --verbose=true --silent=false",
    "prettier": "npx prettier --write \"src/**/*\" \"test/**/*\""
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "mongodb": "^3.5.5",
    "morgan": "~1.9.1",
    "passport": "^0.4.1",
    "passport-google-oauth": "^2.0.0",
    "ramda": "^0.27.0",
    "url-join": "latest",
    "validate.js": "^0.13.1"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.1.3",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.3",
    "@types/faker": "^4.1.10",
    "@types/http-errors": "^1.6.3",
    "@types/jest": "^25.1.4",
    "@types/mongodb": "^3.5.5",
    "@types/morgan": "^1.9.0",
    "@types/node": "^13.9.0",
    "@types/passport": "^1.0.3",
    "@types/passport-google-oauth": "^1.0.41",
    "@types/ramda": "^0.27.0",
    "@types/supertest": "^2.0.8",
    "@types/url-join": "^4.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.6.0",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-prettier": "^3.1.1",
    "faker": "^4.1.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "merge": "^1.2.1",
    "nodemon": "^1.19.4",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-rebase": "npm test"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  },
  "nodemonConfig": {
    "ignore": [
      "*.(test|spec).(js|ts)"
    ],
    "delay": 3000
  }
}
