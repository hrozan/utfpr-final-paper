{
  "name": "fp-server",
  "version": "0.0.0",
  "private": true,
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon -r dotenv/config src/index.ts",
    "server": "node -r dotenv/config dist/server.js",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:dev": "jest --watch --verbose=true --silent=false",
    "kill": "sudo kill -9 $(sudo lsof -t -i:3000)"
  },
  "dependencies": {
    "@koa/cors": "^2.2.3",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.3.0",
    "koa-morgan": "^1.0.1",
    "koa-router": "^8.0.8",
    "mongodb": "^3.5.5",
    "ramda": "^0.27.0"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.1.3",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/faker": "^4.1.10",
    "@types/http-errors": "^1.6.3",
    "@types/jest": "^25.1.4",
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-morgan": "^1.0.4",
    "@types/koa-router": "^7.4.0",
    "@types/mongodb": "^3.5.5",
    "@types/morgan": "^1.9.0",
    "@types/node": "^13.9.0",
    "dotenv": "^8.2.0",
    "faker": "^4.1.0",
    "jest": "^25.1.0",
    "merge": "^1.2.1",
    "nodemon": "^1.19.4",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  },
  "nodemonConfig": {
    "ignore": [
      "*.(test|spec).(js|ts)"
    ],
    "delay": 3000
  }
}
